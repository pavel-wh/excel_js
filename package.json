{
  "name": "excel_js",
  "version": "1.0.0",
  "description": "JavaScript, Webpack, Babel, ESLint, SCSS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --open",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "eslint": "eslint '**/*.js'",
    "eslint:fix": "eslint '**/*.js' --fix",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavel-wh/excel_js.git"
  },
  "keywords": [
    "JavaScript",
    "Webpack",
    "Babel",
    "ESLint",
    "SCSS"
  ],
  "author": "pB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pavel-wh/excel_js/issues"
  },
  "homepage": "https://github.com/pavel-wh/excel_js#readme",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.0",
    "stylelint-config-airbnb": "0.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.17.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
