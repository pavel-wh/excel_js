{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				}
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		[
			"module-resolver",
			{
				"root": ["."
				],
				"alias": {
					"@": "./src",
					"@core": "./src/core"
				}
			}
		]
	]
}
